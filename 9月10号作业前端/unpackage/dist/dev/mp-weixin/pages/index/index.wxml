<view class="content"><image class="logo" src="/static/logo.png"></image><view class="text-area"><text class="title">{{title}}</text></view><view style="background-color:aquamarine;"><input placeholder="用户名" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" value="{{username}}" bindinput="__e"/><view style="height:10rpx;"></view><input maxlength="16" placeholder="密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><view style="margin-top:200rpx;"><button data-event-opts="{{[['tap',[['register',['$event']]]]]}}" bindtap="__e">注册</button><button data-event-opts="{{[['tap',[['land',['$event']]]]]}}" bindtap="__e">登录</button></view></view>